<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<launch>

    <arg name="robotID" default="1"/>


    <group ns="machine_$(arg robotID)">
        <arg name="name" value="fake_position"/>
        <node name="$(arg name)" pkg="fake_position" type="fake_position_node" output="screen"
              args="$(arg robotID)">
        </node>
    </group>

</launch>
